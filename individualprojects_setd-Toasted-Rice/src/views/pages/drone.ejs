<!doctype html>
<html lang="en-us">
<head>
    <meta charset="utf-8">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
	<title><%=page_title %></title>
	<link
		rel="stylesheet"
		href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
		integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
		crossorigin="anonymous"
	/>
	<link rel="shortcut icon" href="/resources/img/drone/favicon.ico">
	<link rel="stylesheet" href="/resources/css/global.css">
	<link rel="stylesheet" href="/resources/css/animations.css">
	<link rel="stylesheet" href="/resources/css/drone.css">
</head>
<body>
	<%- include ('../partials/menu') %>
	<div class="container-fluid py-4" id="content">
		<div id="unity-container" class="unity-desktop">
			<canvas id="unity-canvas" width=960 height=600></canvas>
			<div id="unity-loading-bar">
				<div id="unity-logo"></div>
				<div id="unity-progress-bar-empty">
					<div id="unity-progress-bar-full"></div>
				</div>
			</div>
			<div id="unity-warning"> </div>
			<div id="unity-footer">
				<div id="unity-webgl-logo"></div>
				<div id="unity-fullscreen-button"></div>
				<div id="unity-build-title">DroneDemo</div>
			</div>
			<img src="/resources/img/drone/keyboard.png" id="controls" height="150px">
		</div>
	</div>
</body>
<footer>
	<%- include('../partials/footer') %>
</footer>
</html>